{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:development": "cross-env NODE_ENV=development vue-cli-service build",
    "build:prod": "cross-env NODE_ENV=production vue-cli-service build",
    "test:unit": "cross-env NODE_ENV=test vue-cli-service test:unit",
    "test:e2e": "cross-env NODE_ENV=production vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "report:prod": "cross-env NODE_ENV=production vue-cli-service build --report-json",
    "analyze:prod": "yarn run report:prod && yarn run webpack-bundle-analyzer dist/report.json"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.0.0-beta.16",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@types/jest": "^26.0.19",
    "@types/mocha": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-webdriverio": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "9",
    "chromedriver": "87",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "geckodriver": "^1.21.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "postcss": "7",
    "postcss-custom-properties": "^10.0.0",
    "postcss-import": "12",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "pretty-quick": "^3.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^10.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3",
    "vue-jest": "^5.0.0-0",
    "wdio-chromedriver-service": "^6.0.3",
    "wdio-geckodriver-service": "^1.1.0",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "pretty-quick --staged --pattern \"src/**/*.{ts,tsx}\"",
      "vue-cli-service lint",
      "git add"
    ]
  }
}
